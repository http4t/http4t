    "test-browser": "ts-node test-browser.ts"
  },
  "dependencies": {
    "@http4t/bidi": "0.0.82",
    "@http4t/node": "0.0.82",
    "@types/uuid": "^3.4.7",
    "pg": "7.9.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/pg": "7.4.14"
  }
}
